{
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"root": true,
	"env": {
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"parser": "babel-eslint",
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"semi": ["warn", "never"],
		"no-empty-function": "warn",
		"@typescript-eslint/no-empty-function": "warn"
	}
}
