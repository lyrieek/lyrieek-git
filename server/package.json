{
	"name": "lyrieek-git-server",
	"version": "0.1.0",
	"private": true,
	"bin": "app.js",
	"scripts": {
		"predev": "tsc",
		"dev": "node ../build/index.js",
		"prebuild": "tsc",
		"build": "pkg . --out-path=dist/ && echo ./dist/lyrieek-git-server.exe"
	},
	"dependencies": {
		"core-js": "^3.8.3",
		"execa": "^5.0.0",
		"express": "^4.17.1",
		"express-ws": "^4.0.0"
	},
	"pkg": {
		"options": [
			"experimental-modules"
		],
		"scripts": [
			"./app.js"
		],
		"assets": [
			"../dist"
		],
		"targets": [
			"node14-win-x64"
		]
	},
	"devDependencies": {
		"@types/express-ws": "^3.0.0"
	}
}
