{
	"name": "lyrieek-git-server",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"bin": "app.js",
	"scripts": {
		"predev": "tsc",
		"dev": "node ../build",
		"build": "rollup -c && echo node ../dist/lyrieek-git-server",
		"pkg": "pkg . --out-path=dist/ && echo ../dist/lyrieek-git-server.exe"
	},
	"dependencies": {
		"bufferutil": "^4.0.3",
		"core-js": "^3.8.3",
		"execa": "^5.0.0",
		"express": "^4.17.1",
		"express-ws": "^4.0.0",
		"tslib": "^2.1.0",
		"utf-8-validate": "^5.0.4"
	},
	"pkg": {
		"options": [
			"experimental-modules"
		],
		"scripts": [
			"./app.js"
		],
		"assets": [
			"../dist"
		],
		"targets": [
			"node14-win-x64"
		]
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@rollup/plugin-typescript": "^8.2.0",
		"@types/express-ws": "^3.0.0"
	}
}
